{
    "AnalyzerSettings": {
        "Bins": 200,
        "Rate": 100,
        "Normal": 255
    },
    "Configuration": {
        "DownloadPath": "Downloads",
        "FFMpegPath": "ffmpeg/ffmpeg.exe",
        "Theme": "1",
        "Accent": "#FFFF00FF"
    },
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
        "MinimumLevel": "Debug",
        "WriteTo": [
            { "Name": "Console" },
            {
                "Name": "File",
                "Args": {
                    "path": "Logs/log-.txt",
                    "rollingInterval": "Day"
                }
            }
        ],
        "Enrich": [ "FromLogContext", "WithThreadId" ],
        "Properties": {
            "Application": "S.Player"
        }
    }
}